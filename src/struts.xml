<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd"> 

<struts>  
	<constant name="struts.devMode" value="true"/>
	<package name="com.diworksdev.account.action" extends="struts-default">

<!-- HomeAction -->
		<action name="HomeAction" class="com.diworksdev.account.action.HomeAction" method="execute"> 
			<result name="success">home.jsp</result> 
		</action>	
		
<!-- registAction -->
		<action name="registAction" class="com.diworksdev.account.action.registAction" method="execute">
			<result name="success">regist.jsp</result>
		</action>
		
<!-- regist_confirmAction -->
		<action name="regist_ConfirmAction" class="com.diworksdev.account.action.regist_confirmAction" method="execute">
			<result name="success">regist_confirm.jsp</result>
			<result name="error">regist_confirm.jsp</result>
			<result name="input">regist_confirm.jsp</result>
		</action>
		
<!-- regist_completeAction -->
		<action name="regist_completeCompleteAction" class="com.diworksdev.account.action.regist_completeAction" method="execute">
			<result name="success">regist_complete.jsp</result>
		</action>
		
	</package>
</struts> 